{
  "name": "cleentfaar/slack",
  "description": "A PHP library providing integration with the Slack API and webhooks",
  "keywords": ["slack", "api"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Cas Leentfaar",
      "homepage": "https://github.com/cleentfaar",
      "email": "info@casleentfaar.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "symfony/options-resolver": "~2.2",
    "guzzlehttp/guzzle": "~4.1",
    "guzzlehttp/log-subscriber": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.1"
  },
  "autoload": {
    "psr-0": {
      "CL\\Slack": ""
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "target-dir": "CL/Slack"
}
