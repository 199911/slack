{
  "name": "cleentfaar/slack",
  "description": "A PHP library providing easy access to the Slack API",
  "keywords": [
    "slack",
    "slack api",
    "chat"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Cas Leentfaar",
      "homepage": "https://github.com/cleentfaar",
      "email": "info@casleentfaar.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "jms/serializer": "~0.16",
    "symfony/event-dispatcher": "~2.2",
    "guzzlehttp/guzzle": ">=4.2",
    "doctrine/collections": "~1.2",
    "doctrine/dbal": "~1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.4"
  },
  "suggest": {
    "cleentfaar/slack-bundle": "For easy integration with your Symfony projects",
    "cleentfaar/slack-cli": "Command-line application for interacting with the Slack API"
  },
  "autoload": {
    "psr-0": {
      "CL\\Slack": "src/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "target-dir": "CL/Slack"
}
